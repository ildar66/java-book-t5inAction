<html t:type="layout" xmlns:t="http://tapestry.apache.org/schema/tapestry_5_3.xsd"
        xmlns:p="tapestry:parameter">

    <form t:type="beaneditform" t:id="registerForm" object="user"
          submitLabel="message:submit-value" clientValidation="none"
          model="model">

        <p:passwordConfirmed>
            <t:label for="passwordConfirmed"/>
            <t:passwordfield t:id="passwordConfirmed" value="passwordConfirmed"/>
        </p:passwordConfirmed>

    </form>

</html>
